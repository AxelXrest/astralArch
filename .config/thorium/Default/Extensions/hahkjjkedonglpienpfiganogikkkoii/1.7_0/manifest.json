{
   "background": {
      "scripts": [ "event.js" ]
   },
   "browser_action": {
      "default_icon": "icon128.png",
      "default_title": "Disable"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "*://*/*.m3u8*", "*://*/*.ts*" ]
   } ],
   "description": "Play HLS urls in-browser",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Play HLS",
   "permissions": [ "*://*/*m3u8*", "*://*/*.ts*", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7",
   "web_accessible_resources": [ "*.html" ]
}
